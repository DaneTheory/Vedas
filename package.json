{
  "name": "vedas",
  "version": "1.0.1-0",
  "description": "A hackable, extendable, functionally-composable way to go about dealing with wrangling types in JavaScript. Auto-casting, coercion, and type inference for the Alice in WOnderland world that is JS runtime.",
  "repository": "https://github.com/DaneTheory/Vedas.git",
  "author": "Branden Dane (https://DaneTheory.com) <DaneTheory@gmail.com>",
  "license": "MIT",
  "private": false,
  "source": "src/index.js",
  "main": "dist/index.js",
  "browser": "dist/index.umd.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.module.js"
    }
  },
  "umd:main": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.module.js",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "dev": "microbundle --name vedas watch",
    "test": "yarn lint && jest",
    "coverage": "jest --coverage",
    "watch": "jest --watch",
    "lint": "standard --fix --env jest --parser babel-eslint --verbose | snazzy",
    "build": "yarn clean && yarn lint && microbundle --name vedas",
    "release": "np",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "castit": "^1.2.0",
    "ensurism": "^0.6.0",
    "kind-of": "^6.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "np": "^7.4.0",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "snazzy": "^9.0.0",
    "standard": "^16.0.3",
    "standard-version": "^9.1.1"
  },
  "keywords": [
    "danetheory",
    "types",
    "type",
    "cast",
    "coerce",
    "coercian",
    "auto-cast",
    "type-inference",
    "infer",
    "auto",
    "strings"
  ]
}
